    "distributions": {
        "exp":
            {"fun": "(-0.8696 + 2.087*math.exp(({distance})*0.0031))*{value}", "__comment": "distribution based on Kole et al. 2006"},
        "decay":
            {"fun": "math.exp({distance}*{constant})*{value}", "parameters": ["constant"]},
        "rising":
            {"fun": "(1-math.exp({distance}*{constant_rise}))*{value}", "parameters": ["constant_rise"]},
        "bimodal":
            {"fun": "({distance}<=10)*(1-math.exp({distance}*{constant_bimod1}))*{value} + ({distance}>10)*(math.exp({distance}*{constant_bimod2})*{value})", "parameters": ["constant_bimod1","constant_bimod2"]},
        "guassian_nav12":
            {"fun": "(0.258 + 0.43*math.exp(-0.5*(({distance}-11.605)/6.964)*(({distance}-11.605)/6.964)))*{value}","soma_ref_point":1.0, "__comment": "Guassian based on decrease of Nav1.2 in AIS, Hu et al., 2009"},
